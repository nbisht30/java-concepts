import java.io.BufferedReader;
import java.io.FileInputStream;
import java.io.IOException;
import java.io.InputStreamReader;

public class CollectionsInterviewQuestion {
	public static void main(String[] args) {
		String data;
		try (BufferedReader in = new BufferedReader(new InputStreamReader(new FileInputStream(""), "UTF-8"))){
			while ((data = in.readLine()) != null && !data.equals("start")) {
				System.out.print("\nDid not enter \"start\". Try again: ");				
			}
		} catch (IOException e) {
			e.printStackTrace();
		}
	}
}
